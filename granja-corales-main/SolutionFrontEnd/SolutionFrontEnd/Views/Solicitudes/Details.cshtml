@model SolutionFrontEnd.Models.Solicitudes

@{
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
    ViewData["Title"] = "Detalle solicitud";
}

<h1>@ViewData["Title"]</h1>

<div>
    <h4>Solicitudes</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FecCreacion)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FecCreacion)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.EstadoSolicitud)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EstadoSolicitud)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdUsuario)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdUsuario)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.IdSolicitud">Editar</a> |
    <a class="btn btn-secondary" asp-action="Index">Regresar</a>
</div>
